{
  "name": "js-dev-environment",
  "version": "2.0.0",
  "author": "jeremy@integratingmagic.io",
  "license": "ISC",
  "description": "a generic development enviornment for developing JS in FileMaker",
  "scripts": {
  "start": "parcel src/index.html --no-source-maps",
  "build": "parcel build src/index.html --no-source-maps --public-url ./",
  "inline": "node ./scripts/inline.js",
  "upload": "node ./scripts/upload",
  "build:upload": "npm run build && npm run inline && npm run upload",
  "deploy-to-fm": "npm run build && npm run inline && npm run upload",
  "openfile": "node ./scripts/openFileOnServer",
  "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "html-webpack-plugin": "^5.6.3",
    "open": "^8.2.1",
    "parcel": "^2.13.3",
    "postcss": "^8.5.3",
    "prettier": "2.6.2",
    "process": "^0.11.10",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.1.8",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.1",
    "@mui/x-data-grid-premium": "^7.24.1",
    "@mui/x-license": "^7.24.1",
    "@reduxjs/toolkit": "^2.5.0",
    "c3": "^0.7.20",
    "canvasjs-react-charts": "^1.0.5",
    "csv-parser": "^3.2.0",
    "fs": "^0.0.1-security",
    "papaparse": "^5.5.1",
    "react": "^18.2.0",
    "react-apexcharts": "^1.7.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router": "^6.20.0",
    "react-router-dom": "^6.20.0",
    "styled-components": "^6.1.14",
    "xlsx": "^0.18.5"
  }
}
